 354:mcp2515.c     **** 			spi_putc(message->data[i]);
 355:mcp2515.c     **** 		}
 356:mcp2515.c     **** 	}
 357:mcp2515.c     **** 	SET(MCP2515_CS);
 358:mcp2515.c     **** 	
 359:mcp2515.c     **** 	_delay_us(1);
 360:mcp2515.c     **** 	
 361:mcp2515.c     **** 	// send message
