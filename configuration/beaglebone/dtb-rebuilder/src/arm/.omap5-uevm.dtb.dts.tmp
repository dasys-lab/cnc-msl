  bandgap: bandgap@4a0021e0 {
   reg = <0x4a0021e0 0xc
          0x4a00232c 0xc
          0x4a002380 0x2c
          0x4a0023C0 0x3c>;
   interrupts = <0 126 4>;
   compatible = "ti,omap5430-bandgap";

   #thermal-sensor-cells = <1>;
