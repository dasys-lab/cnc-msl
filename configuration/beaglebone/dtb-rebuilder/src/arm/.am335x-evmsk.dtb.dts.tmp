  phy1 = &usb1_phy;
  ethernet0 = &cpsw_emac0;
  ethernet1 = &cpsw_emac1;
 };

 cpus {
  #address-cells = <1>;
  #size-cells = <0>;
  cpu@0 {
   compatible = "arm,cortex-a8";
   device_type = "cpu";
   reg = <0>;

   voltage-tolerance = <2>;

   clocks = <&dpll_mpu_ck>;
   clock-names = "cpu";

   clock-latency = <300000>;
  };
 };

 pmu {
  compatible = "arm,cortex-a8-pmu";
  interrupts = <3>;
 };





 soc {
  compatible = "ti,omap-infra";
